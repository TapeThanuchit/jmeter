              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">import groovy.json.JsonOutput

// Retrieve a value from a variable in JMeter.
String reasonCode = vars.get(&quot;reason_code&quot;)
String reasonDesc = vars.get(&quot;reason_desc&quot;)
String[] reason_code = reasonCode ? reasonCode.split(&quot;,&quot;) : []  // Check the value first.
String[] reason_desc = reasonDesc ? reasonDesc.split(&quot;,&quot;) : []  // Check the value first.
int reason_code_size = reason_code.length

String blacklistFieldName = vars.get(&quot;field_name&quot;)
String blacklistValue = vars.get(&quot;value&quot;)
String[] field_name = blacklistFieldName ? blacklistFieldName.split(&quot;,&quot;).collect {it .trim()} : []  // Check the value first.
String[] value = blacklistValue ? blacklistValue.split(&quot;,&quot;) : []  // Check the value first.
int field_name_size = field_name.length

// Prepare a List to store JSON data
def reasonsList = []
def blacklistsList = []

// Create a list of Reason JSON
for(int i = 0; i &lt; reason_code_size; i++){
    def reason = [
        &quot;reason_code&quot;: reason_code[i],
        &quot;reason_desc&quot;: reason_desc[i]
    ]
    reasonsList.add(reason)
}

// Create a list of Blacklist JSON.
for(int i = 0; i &lt; field_name_size; i++){
    def blacklist = [
        &quot;field_name&quot;: field_name[i],
        &quot;value&quot;: value[i]
    ]
    blacklistsList.add(blacklist)
}

// Convert to JSON String.
String blacklistsJson = JsonOutput.toJson(blacklistsList)
String reasonsJson = JsonOutput.toJson(reasonsList)

// Store the result in a JMeter variable.
vars.put(&quot;blacklistsJson&quot;, blacklistsJson)
vars.put(&quot;reasonsJson&quot;, reasonsJson)</stringProp>

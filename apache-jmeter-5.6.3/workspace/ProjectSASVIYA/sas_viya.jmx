              <stringProp name="script">import groovy.json.JsonOutput

// Retrieve a value from a variable in JMeter.
String conditionsFieldName = vars.get(&quot;field_name&quot;) 
String conditionOper = vars.get(&quot;oper&quot;)
String conditionValue = vars.get(&quot;value&quot;)

String[] field_name = conditionsFieldName ? conditionsFieldName.split(&quot;,&quot;) : [] // Check the value first.
String[] oper = conditionOper ? conditionOper.split(&quot;,&quot;) : [] // Check the value first.
String[] value = conditionValue ? conditionValue.split(&quot;,&quot;) : [] // Check the value first.
int field_name_size = field_name.length


// Prepare a List to store JSON data
def conditionsList = []

// Create a list of Blacklist JSON
for(int i = 0; i &lt; field_name_size; i++){
    def condition = [
        &quot;field_name&quot;: field_name[i],
        &quot;oper&quot;: oper[i],
        &quot;value&quot;: value[i]
    ]
    conditionsList.add(condition)
}


// Convert to JSON String.
String conditionslistJton = JsonOutput.toJson(conditionsList)

// Store the result in a JMeter variable.
vars.put(&quot;conditionslistJton&quot;, conditionslistJton)</stringProp>
